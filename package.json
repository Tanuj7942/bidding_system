    "dotenv-safe": "^8.2.0",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
